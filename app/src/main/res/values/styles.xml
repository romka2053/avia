<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="style_title1">
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="text_title2">
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="text1">
        <item name="android:fontFamily">@font/sf_pro_display_regular</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="text_button_view_all">
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textColor">@color/blue_other</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="text_name">
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_description">
        <item name="android:fontFamily">@font/sf_pro_display_regular</item>
        <item name="android:textColor">@color/grey_5</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text2">
        <item name="android:fontFamily">@font/sf_pro_display_regular</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="text_in_chip" parent="TextAppearance.MaterialComponents.Chip">
        <item name="android:fontFamily">@font/sf_pro_display_medium_italic</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_medium_14">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_medium_italic_14">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium_italic</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_medium_italic_14_grey6">
        <item name="android:textColor">@color/grey_6</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium_italic</item>
        <item name="android:textSize">14sp</item>

    </style>


    <style name="text_medium_italic_14_blue_other">
        <item name="android:fontFamily">@font/sf_pro_display_medium_italic</item>
        <item name="android:textColor">@color/blue_other</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="input_search">
        <item name="android:background">@android:color/transparent</item>
        <item name="fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="hintTextColor">@color/grey_6</item>
        <item name="android:inputType">textCapWords</item>
    </style>


    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">@color/red</item>
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="CalenderViewDateCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/white</item>
        <item name="android:weekNumberColor">@color/red</item>
    </style>

    <style name="CalenderViewWeekCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/white</item>
    </style>


    <style name="CustomAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@color/grey_2</item>

    </style>

    <style name="button_all_ticket">

        <item name="android:background">@drawable/button_all_tickets</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/sf_pro_display_semibold_italic</item>
        <item name="android:textSize">16sp</item>


    </style>

    <style name="badge">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium_italic</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/badge_background</item>
    </style>


    <style name="button_bottom_left">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/button_bottom_left</item>
    </style>

    <style name="button_bottom_right">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sf_pro_display_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/button_bottom_right</item>
    </style>


    <style name="BottomNavigation">
        <item name="android:background">@drawable/bottom_menu_background</item>

        <item name="itemIconTint">@color/selector_bottom</item>
        <item name="itemTextColor">@color/selector_bottom</item>
    </style>



</resources>